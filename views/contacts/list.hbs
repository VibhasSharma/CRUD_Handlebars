<table class="table table-striped">
    <thread>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>eMail</th>
            <th>Date of Birth</th>
            <th>Company</th>
            <th></th>
        </tr>
    </thread>
    <tbody>
        {{#each list}}
        <tr>
            <td>{{this.fName}}</td>
            <td>{{this.lName}}</td>
            <td>{{this.email}}</td>
            <td>{{this.dob}}</td>
            <td>{{this.company}}</td>
            <td>
                <a><i class="fa fa-pencil fa-lg" aria-hidden="true"></i></a>
                <a><i class="fa fa-trash fa-lg" aria-hidden="true"></i></a>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>